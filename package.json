		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"prettier": "^3.5.0",
		"prettier-plugin-svelte": "^3.3.3",
		"sass-embedded": "^1.83.4",
		"svelte": "^5.19.9",
		"svelte-check": "^4.1.4",
		"typescript": "^5.7.3",
		"typescript-eslint": "^8.24.0",
		"vite": "^5.4.14"
